{
  "name": "hla-cdk",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "hla-cdk": "bin/hla.ts"
  },
  "scripts": {
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "cdk:bootstrap": "cdk bootstrap aws://$env:AWS_ACCOUNT_ID/$env:AWS_REGION --profile $env:AWS_PROFILE",
    "cdk:deploy": "cdk deploy HLAStack --require-approval never --profile $env:AWS_PROFILE",
    "cdk:destroy": "cdk destroy --all --force --profile $env:AWS_PROFILE"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "aws-cdk": "2.152.0",
    "constructs": "10.3.0",
    "source-map-support": "^0.5.21",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.152.0"
  }
}
